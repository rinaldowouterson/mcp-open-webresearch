# Test Environment Configuration
NODE_ENV=test

# Proxy Testing (set to false for most tests)
USE_PROXY=false
HTTP_PROXY=
HTTPS_PROXY=
SOCKS5_PROXY=
SOCKS4A_PROXY=
SOCKS4_PROXY=
NODE_EXTRA_CA_CERTS=

# CORS Testing
ENABLE_CORS=true
CORS_ORIGIN=http://localhost:3000

# Debug for tests
WRITE_DEBUG_FILE=false
WRITE_DEBUG_TERMINAL=false

# Test timeouts
TEST_TIMEOUT=60000