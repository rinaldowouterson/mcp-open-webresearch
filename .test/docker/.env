CERT_HOST_FOR_STORE=../../certs/store

# Node.js Environment Mode
# NODE_ENV=production
# PORT=3000

# CORS Configuration
# ENABLE_CORS=false
# CORS_ORIGIN="*"

# Proxy Configuration
# ENABLE_PROXY=false
# SOCKS5_PROXY=
# HTTPS_PROXY=
# HTTP_PROXY=

# SSL/TLS Configuration
# NODE_TLS_REJECT_UNAUTHORIZED=1
# NODE_USE_SYSTEM_CA=0

# Debug Configuration
# DRY_RUN=false
# WRITE_DEBUG_TERMINAL=false
# WRITE_DEBUG_FILE=false
# MCP_LOG_PATH=./mcp-debug.log

# Search Engine Configuration
# Valid values: bing, duckduckgo, brave
# DEFAULT_SEARCH_ENGINES=bing,duckduckgo,brave

# LLM Sampling Configuration (Experimental)
# SAMPLING=false
# OPENAI_API_KEY=
# OPENAI_BASE_URL="https://openrouter.ai/api/v1"
# OPENAI_MODEL="google/gemini-2.0-flash-001"

# Docker specific
# DOCKER_ENVIRONMENT=false
# CHROMIUM_EXECUTABLE_PATH=
